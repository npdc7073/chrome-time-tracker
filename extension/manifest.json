{
    "manifest_version": 3,
    "name": "Time Tracker & Productivity Analytics",
    "version": "1.0",
    "permissions": [
      "tabs",
      "windows",
      "storage",
      "scripting"
    ],
    "background": {
      "service_worker": "background.js"
    },
    "action": {
      "default_popup": "popup.html",
      "default_icon": {
        "128": "icons/icon128.png"
      }
    },
    "icons": {
      "128": "icons/icon128.png"
    }
  }
  